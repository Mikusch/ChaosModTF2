screenspace_general
{
	$pixshader "chaos_scroll_ps20"

	$basetexture "_rt_FullFrameFB"
	$texture1    ""
	$texture2    ""
	$texture3    ""

	$x360appchooser 1
	$ignorez        1
	$fix_fb         32768
	"<dx90"
	{
		$no_draw 1
	}

	$copyalpha                 1
	$alpha_blend_color_overlay 0
	$alpha_blend               0
	$linearwrite               0
	$linearread_basetexture    0
	$linearread_texture1       0
	$linearread_texture2       0
	$linearread_texture3       0

	$c0_x     0.0 // time
	$c0_y     1.0 // direction; 0 for X axis, 1 for Y axis
	$c0_z     0.1 // speed. negative for backwards

	Proxies
	{
		Equals
		{
			srcVar1			$fix_fb
			resultVar		$flags2
		}
		CurrentTime
		{
			resultVar		$c0_x
		}
	}
}
